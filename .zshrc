plugins=(git)

export PATH="$PATH:/Users/$USER/bin:/Users/$USER/.vimpkg/bin"
export PATH="$PATH:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbinr:/usr/local/bin/dot"

