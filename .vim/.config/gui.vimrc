hi vertsplit guifg=bg guibg=bg
colorscheme atom-dark
set guifont=InputMono:h16       "Set font type and size. Depends on the resolution. Larger screens, prefer h20
set guioptions-=l
set guioptions-=L
set guioptions-=r
set guioptions-=R
set lines=100                   "This helps to start mvim maximized
set columns=450                 "This helps to start mvim maximized
"set macligatures
"set guifont=Fira\ Code:h16
